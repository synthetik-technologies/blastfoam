FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    symmetricY
    {
        type            symmetry;
    }

    symmetricZ
    {
        type            symmetry;
    }

    symmetricX
    {
        type            symmetry;
    }

    free
    {
        type            zeroGradient;
    }
}


