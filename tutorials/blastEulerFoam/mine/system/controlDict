writeInterval   5e-5;

cycleWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general; // fixed; // using precision below --- better for paraview

timePrecision   12;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       1e-5;

