FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101298;

boundaryField
{
    ground
    {
        type            zeroGradient;
    }
    midPlane1
    {
        type            symmetry;
    }
    midPlane2
    {
        type            symmetry;
    }
    outlet
    {
        type            zeroGradient;
    }
}


