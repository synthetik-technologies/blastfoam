x 16.0;

vertices
(
    (0      0.0     0) // bottom
    ($x     0.0     0)
    ($x     $x      0)
    (0      $x      0)
    (0      0       $x)
    ($x     0       $x)
    ($x     $x      $x)
    (0      $x      $x)
);

blocks
(
	 hex (0 1 2 3 4 5 6 7) (20 20 20) simpleGrading (1 1 1)
);

edges
(
);

