FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.001;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.001;
    }
    bottom
    {
        type            symmetryPlane;
    }
    top
    {
        type            symmetryPlane;
    }
    obstacle
    {
        type            blast::alphatWallFunction;
        value           uniform 0.001;
    }
    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
