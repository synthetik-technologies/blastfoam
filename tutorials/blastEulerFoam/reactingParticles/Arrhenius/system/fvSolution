FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|alpahRhoU|alphaRhoE|alpha).*"
    {
        solver          diagonal;
    }

    "(U|e|Yi).*"
    {
        solver          PBiCGStab;
        preconditioner  DIC;
        minIter         1;
        tolerance       1e-09;
        relTol          0;
    }

    "Theta.*"
    {
        $U;
        tolerance       1e-10;
        relTol          0;
    }
}


