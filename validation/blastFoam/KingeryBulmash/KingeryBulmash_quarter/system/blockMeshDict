(
    ground
    {
        type patch;
        faces
        (
            (0 1 5 4)
        );
    }
    outlet
    {
        type patch;
        faces
        (

            (4 5 6 7)
            (1 2 6 5)
            (2 3 7 6)
        );
    }
    symmetryPlanes
    {
        type symmetry;
        faces
        (
            (0 1 2 3)
            (0 3 7 4)
        );
    }
);

