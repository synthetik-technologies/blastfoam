FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "system";
  object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     blastFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.040;

writeControl    adjustableRunTime;

writeInterval   1;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   12;

runTimeModifiable true;

adjustTimeStep  yes;

deltaT          1e-6;

maxCo           0.5;

// ************************************************************************* //


functions
{
    writeTimes
    {
        type writeTimeList;
        times (0.01 0.016 0.026 0.028 0.03 0.032 0.034 0.038);
    }
}
