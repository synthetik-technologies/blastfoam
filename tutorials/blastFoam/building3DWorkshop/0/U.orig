FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    walls
    {
        type            slip;
    }

    outlet
    {
        type            zeroGradient;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }

    ground
    {
        type            slip;
    }
}

