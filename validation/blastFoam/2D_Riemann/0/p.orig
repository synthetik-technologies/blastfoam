FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.4;

boundaryField
{
    top
    {
        type            zeroGradient;
        value           $internalField;
    }
    bottom
    {
        type            fixedValue;
        value           uniform 1.0;
    }
    left
    {
        type            fixedValue;
        value           uniform 1.0;
    }
    right
    {
        type            fixedValue;
        value           uniform 1.0;
    }

    defaultFaces
    {
        type            empty;
    }
}

