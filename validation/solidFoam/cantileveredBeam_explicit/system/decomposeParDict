FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  4;

method          scotch;

simpleCoeffs
{
    n          ($numberOfSubdomains 1 1);
    delta       0.001;
}

hierarchicalCoeffs
{
    n           (1 $numberOfSubdomains 1);
    delta       0.001;
    order       xyz;
}

scotchCoeffs
{
}

manualCoeffs
{
    dataFile    "";
}

distributed     no;

roots           ( );

