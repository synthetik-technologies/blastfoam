FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (copper gas);

copper
{
    type basic;
    thermoType
    {
        transport   const;
        thermo      eConst;
        equationOfState CochranChan;
    }

    equationOfState
    {
        Gamma0      2.0;
        A           1.45667e11;
        B           1.4775e11;
        Epsilon1    2.994;
        Epsilon2    1.994;
        rho0        8900;
    }
    specie
    {
        molWeight   63.546;
    }
    transport
    {
        mu              0;              // Viscosity
        Pr              1;              // Prandtl number
    }
    thermodynamics
    {
        Cv          3860;           // Heat capacity
        Hf          0.0;
    }

    residualRho 1e-10;
    residualAlpha 1e-10;
}

gas
{
    type basic;
    thermoType
    {
        transport   const;
        thermo      eConst;
        equationOfState JWL;
    }

    equationOfState
    {
        omega       0.25;
        A           854.5e9;
        B           20.510e9;
        R1          4.6;
        R2          1.35;
        rho0        1840.0;
    }

    specie
    {
        molWeight       28.97;
    }
    transport
    {
        mu              0;              // Viscosity
        Pr              1;              // Prandtl number
    }
    thermodynamics
    {
        type        eConst;
        Cv          718;           // Heat capacity
        Hf          0.0;
    }

    activationModel none;

    residualRho 1e-10;
    residualAlpha 1e-10;
}


