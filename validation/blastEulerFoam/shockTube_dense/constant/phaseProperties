FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      MieGruneisenEOSProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (particles gas);

particles
{
    phaseType        granular;
    diameterModel constant;
    constantDiameterCoeffs
    {
        d           5e-6;
    }

    alphaMax    0.65;
    alphaMinFriction 0.5;

    type basic;
    thermoType
    {
        equationOfState rhoConst;
        thermo  eConst;
        transport constIso;
    }
    specie
    {
        molWeight 100;
    }
    equationOfState
    {
        rho0         1470;
    }

    transport
    {
        kappa       1.0;
    }
    thermodynamics
    {
        type        eConst;
        Cv          987;           // Heat capacity
        Hf          0.0;
    }

    kineticTheoryCoeffs
    {
        e   0.999;
    }

    residualRho 0;
    residualAlpha 1e-10;
}

gas
{
    phaseType            fluid;
    diameterModel constant;
    constantDiameterCoeffs
    {
        d           0;
    }

    type basic;
    thermoType
    {
        transport   const;
        thermo      eConst;
        equationOfState idealGas;
    }
    equationOfState
    {
        gamma           1.4;            // Heat capactiy ratio
    }
    specie
    {
        molWeight       28.97;
    }
    transport
    {
        mu              1.81e-5;
        Pr              1;
    }
    thermodynamics
    {
        type        eConst;
        Cv          718;
        Hf          0;
    }

    residualRho     1e-6;
    residualAlpha   1e-10;
}

interfacialPressure
{
    type single;
    phase gas;
}
interfacialVelocity
{
    type single;
    phase particles;
}

aspectRatio
(
);

drag
(
    (particles in gas)
    {
        type GidaspowErgunWenYu;
        residualRe 1e-3;
        swarmCorrection
        {
            type none;
        }
    }
);

virtualMass
(
);

heatTransfer
(
    (particles in gas)
    {
        type RanzMarshall;
    }
);

lift
(
    (particles in gas)
    {
        type constantCoefficient;
        Cl 0.5;
    }
);

wallLubrication
(
);

turbulentDispersion
(
);

kineticTheory
{
    residualAlpha       1e-10;

    radialModel SinclairJackson;
    viscosityModel Gidaspow;
    conductivityModel Gidaspow;
    granularPressureModel Lun;
    frictionalStressModel JohnsonJackson;

    JohnsonJacksonCoeffs
    {
        alphaMinFriction        0.5;
        Fr                      0.1;
        eta                     2;
        p                       5;
        phi                     28.5;
        alphaDeltaMin           1e-6;
    }
}
